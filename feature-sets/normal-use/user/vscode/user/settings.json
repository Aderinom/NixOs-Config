{
  "remote.SSH.useLocalServer": false,
  "remote.SSH.useExecServer": true,
  "remote.SSH.enableRemoteCommand": true,
  "editor.fontFamily": "'DroidSansM Nerd Font', 'monospace', monospace",
  "explorer.confirmDelete": false,
  "tabnine.experimentalAutoImports": true,
  "git.autorefresh": true,
  "dev.containers.mountWaylandSocket": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[java]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "hediet.vscode-drawio.resizeImages": null,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "remote.autoForwardPortsSource": "hybrid",
  "remote.SSH.enableAgentForwarding": true,
  "rustMacroExpand.displayCargoCommand": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "git.confirmSync": false,
  "git.replaceTagsWhenPull": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "redhat.telemetry.enabled": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "rust-analyzer.rustfmt.rangeFormatting.enable": true,
  "rust-analyzer.inlayHints.closureStyle": "rust_analyzer",
  "rust-analyzer.restartServerOnConfigChange": true,
  "rust-analyzer.cargo.buildScripts.useRustcWrapper": true,
  "rust-analyzer.check.command": "check",
  "rust-analyzer.check.allTargets": true,
  "rust-analyzer.check.workspace": true,
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.cargo.targetDir": true,
  "rust-analyzer.highlighting.strings": false,
  "rust-analyzer.semanticHighlighting.strings.enable": false,
  "nix.enableLanguageServer": true,
  "nix.hiddenLanguageServerErrors": [
    "textDocument/definition"
  ],
  "nix.serverPath": "nixd",
  "nix.formatterPath": "alejandra",
  "nix.serverSettings": {
    "nixd": {
      "formatting": {
        "command": [
          "alejandra"
        ]
      }
    }
  },
  "workbench.sideBar.location": "right",
  "chat.commandCenter.enabled": false,
  "explorer.autoReveal": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autoHide.mode": "manual",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
  },
  "git.blame.editorDecoration.enabled": true,
  "git.blame.editorDecoration.template": "(${authorDateAgo}) ${authorName} : ${subject}",
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/.*": false
  },
  "cSpell.enabledNotifications": {
    "Maximum Word Length Exceeded": false
  },
  "biome.suggestInstallingGlobally": false,
  "remote.SSH.remotePlatform": {
    "workhost": "linux",
    "strato-tail": "linux"
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.indentSize": "tabSize",
  "editor.insertSpaces": true,
  "github.copilot.nextEditSuggestions.enabled": false,
  "rust-analyzer.assist.preferSelf": true,
  "rust-analyzer.completion.autoimport.exclude": [
    "anyhow::Ok",
    {
      "path": "core::borrow::Borrow",
      "type": "methods"
    },
    {
      "path": "core::borrow::BorrowMut",
      "type": "methods"
    }
  ],
  "rust-analyzer.lens.references.adt.enable": true,
  "rust-analyzer.lens.references.trait.enable": true,
  "rust-analyzer.lens.references.method.enable": true,
  "rust-analyzer.lens.references.enumVariant.enable": true,
  "rust-analyzer.statusBar.clickAction": "stopServer",
  "rust-analyzer.completion.callable.snippets": "add_parentheses",
  "githubPullRequests.pullBranch": "never",
  "github.copilot.nextEditSuggestions.enabled": true,
  "rust-analyzer.rustfmt.extraArgs": [
    "+nightly-2025-07-07"
  ],
  "cue.formatTool": "cue fmt"
}